<h1>Logs#new</h1>
<p>Find me in app/views/logs/new.html.erb</p>


<%= form_for @log, url: new_log_form_path, method: "post" do |f| %>

  <div>
    <%= f.text_field :title, placeholder: "Title" %><br>
    <%= f.text_area :body %>
  </div>

    <%= f.hidden_field :author, :value => @user.email %>
    <%= f.hidden_field :city, :value => @city.city %>
  

    <%= f.submit %>

<% end %>